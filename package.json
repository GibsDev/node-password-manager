{
  "name": "password-manager",
  "version": "1.0.0",
  "description": "A NodeJS password manager server",
  "main": "server.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production npx webpack",
    "start:dev": "npm run githooks && concurrently -n \"webpack,server\" \"cross-env NODE_ENV=development npx webpack\" \"cross-env NODE_ENV=development nodemon server.js\"",
    "start": "node server.js",
    "githooks": "git config core.hooksPath .githooks"
  },
  "author": "GibsDev",
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "prompt": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "bootstrap": "^4.6.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "html-loader": "^1.3.2",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^1.3.5",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-loader": "^2.4.7",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "database/*",
      "utilities/*"
    ]
  }
}
